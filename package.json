    "dotenv": "^8.1.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jwt-simple": "0.5.6",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.9.1",
    "pm2": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.19.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "uuid": "^3.1.0",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
