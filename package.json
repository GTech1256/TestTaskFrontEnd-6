{
  "name": "test-task-frond-end-5",
  "version": "1.2.2",
  "description": "Vue + Express + Sequelize + TypeScript",
  "author": "Roman Bakakin <pomah156@gmail.com>",
  "main": "src/server/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "precommit": "yarn lint",
    "start": "cross-env NODE_ENV=production pm2 start ./src/server/index.ts",
    "dev:server": "nodemon --config ./nodemon.config.json",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive src/server/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/server.api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 src/server/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/server/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/index.html",
    "validate": "yarn lint && yarn test",
    "docs": "apidoc -i src/server -o docs",
    "db:migrate": "npx sequelize-cli db:migrate --config src/server/config/sequelizeOptions.json",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all --config src/server/config/sequelizeOptions.json",
    "db:seed": "npx sequelize-cli db:seed:all --config src/server/config/sequelizeOptions.json",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all --config src/server/config/sequelizeOptions.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:GTech1256/TestTaskFrontEnd-6.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "sequelize",
    "postgresql",
    "es6",
    "es7",
    "es8",
    "es2017",
    "REST",
    "API",
    "typescript"
  ],
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/validator": "^10.11.3",
    "axios": "^0.19.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.17.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv": "^8.1.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jwt-simple": "0.5.6",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.9.1",
    "pm2": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.19.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "uuid": "^3.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "^0.0.44",
    "@types/jest": "^24.0.18",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.9",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "apidoc": "^0.17.5",
    "autoprefixer": "^9.6.1",
    "axios-mock-adapter": "^1.17.0",
    "core-js": "3",
    "coveralls": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.11.0",
    "nyc": "^12.0.2",
    "pg": "^7.12.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
