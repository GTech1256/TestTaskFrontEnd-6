    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.4",
    "jest-serializer-vue": "^2.0.2",
