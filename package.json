    "@babel/preset-env": "^7.6.2",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "^0.0.44",
