{
  "name": "test-task-frond-end-5",
  "version": "1.2.2",
  "description": "Vue + Express + Sequelize + TypeScript",
  "author": "Roman Bakakin <pomah156@gmail.com>",
  "main": "src/server/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "serve:front": "vue-cli-service serve",
    "build:front": "vue-cli-service build",
    "lint:front": "vue-cli-service lint",
    "test:front:e2e": "vue-cli-service test:e2e",
    "test:front:unit": "vue-cli-service test:unit",
    "build:server": "tsc -p tsconfig.server.build.json",
    "watch": "nodemon -e ts -w ./src/server -x npm run watch:server",
    "serve:server": "ts-node src/server/index.ts",
    "precommit": "yarn lint",
    "start:server": "cross-env NODE_ENV=production pm2 start ./dist/server/index.js",
    "start:evennode": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "dev:server": "yarn watch",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test:server": "cross-env NODE_ENV=test jest src/server",
    "test:front": "cross-env NODE_ENV=test jest src/front",
    "test:unit": "cross-env NODE_ENV=test mocha src/server.api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 src/server/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/server/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/index.html",
    "validate": "yarn lint && yarn test",
    "docs": "apidoc -i src/server -o docs",
    "db:migrate": "sequelize db:migrate --config src/server/config/sequelizeOptions.json",
    "db:migrate:undo": "sequelize db:migrate:undo:all --config src/server/config/sequelizeOptions.json",
    "db:seed": "sequelize db:seed:all --config src/server/config/sequelizeOptions.json",
    "db:seed:undo": "sequelize db:seed:undo:all --config src/server/config/sequelizeOptions.json",
    "build": "yarn build:front && yarn build:server",
    "start": "yarn build && start:server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:GTech1256/TestTaskFrontEnd-6.git"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "sequelize",
    "postgresql",
    "es6",
    "es7",
    "es8",
    "es2017",
    "REST",
    "API",
    "typescript"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.2",
